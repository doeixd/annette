{
  "name": "annette",
  "version": "0.0.5",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "keywords": [
    "pridepack"
  ],
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@types/uuid": "^11.0.0",
    "pridepack": "^2.6.4",
    "standard-version": "^9.5.0",
    "tslib": "^2.8.1",
